{
	"name": "es/base-bundle",
	"description": "The Base Cameleon bundle",
	"keywords": ["cameleon", "base"],
	"homepage": "https://github.com/Entropic-Synergies/ESBaseBundle",
	"type": "symfony-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Arthur de Moulins",
			"email": "ademoulins@entropic-group.com"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"symfony/assetic-bundle": "~2.8",
		"doctrine/migrations": "1.5.0",
		"doctrine/doctrine-migrations-bundle": "1.3.",
		"twig/extensions": "~1.0",
		"braincrafted/bootstrap-bundle": "~2.0",
		"knplabs/knp-menu": "2.1.*@dev",
		"knplabs/knp-menu-bundle": "2.1.*@dev",
		"pagerfanta/pagerfanta": "1.0.*@dev",
		"white-october/pagerfanta-bundle": "*",
		"liip/imagine-bundle": "1.0.*@dev",
		"presta/sitemap-bundle": "1.3.*@dev",
		"stof/doctrine-extensions-bundle": "~1.1@dev"
	},
	"autoload": {
		"psr-0": {
			"ES\\Bundle\\BaseBundle": ""
		}
	},
	"bin": ["Resources/bin/update_composer_src"],
	"target-dir": "ES/Bundle/BaseBundle",
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
